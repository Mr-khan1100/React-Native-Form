{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@redux/*": ["src/redux/*"],
      "@components/*": ["src/components/*"],
      "@assets/*": ["./src/assets/Images/*"],
      "@utils/*": ["./src/utils/*"],
      "@services/*": ["./src/services/*"],
      "@constants/*": ["./src/constants/*"],
      "@context/*": ["./src/context/*"],
    }
  }
}